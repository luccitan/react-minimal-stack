{
  "name": "react-minimal-stack",
  "version": "0.1.0",
  "description": "Personal minimal stack to quickly start a React project (with HMR)",
  "main": "server.js",
  "scripts": {
    "dev": "node -r babel-register server.js",
    "wp": "webpack --config webpack.config.prod.babel.js --progress",
    "build": "npm run wp && cp index.html dist/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luccitan/react-minimal-stack.git"
  },
  "bugs": {
    "url": "https://github.com/luccitan/react-minimal-stack/issues"
  },
  "keywords": [
    "react",
    "hmr",
    "stack",
    "hot",
    "webpack",
    "babel"
  ],
  "author": "Tanguy LUCCI <tanguylucci@protonmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.18.2",
    "webpack": "^3.3.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "baobab": "^2.4.3",
    "baobab-react": "^2.1.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ]
  }
}
